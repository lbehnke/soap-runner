<assembly>
  <id>all</id>
  <formats>
    <format>zip</format>
  </formats>
  <baseDirectory></baseDirectory>
  <fileSets>
    <fileSet>
      <directory>target</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>soaprunner.jar</include>
        <include>*-sources.jar</include>
      </includes>
    </fileSet>
    <fileSet>
      <filtered>true</filtered>
      <includes>
        <include>readme*</include>
        <include>license*</include>
      </includes>
    </fileSet>
    <fileSet>
      <filtered>false</filtered>
      <directory>src/main/config</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>replacement.properties</include>
        <include>extractor.properties</include>
      </includes>
    </fileSet>
    
  </fileSets>
</assembly>
